{
  "name": "es-quotes",
  "version": "0.2.0",
  "publisher": "vilicvane",
  "repository": {
    "type": "git",
    "url": "https://github.com/vilic/vscode-es-quotes.git"
  },
  "engines": {
    "vscode": "0.10.x"
  },
  "activationEvents": [
    "onCommand:esQuotes.switchToTemplateString",
    "onCommand:esQuotes.switchToNormalString",
    "onCommand:esQuotes.switchBetweenSingleDoubleQuotes"
  ],
  "main": "./bld/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "ES Quotes Configuration",
      "properties": {
        "esQuotes.defaultQuote": {
          "type": "string",
          "default": "'",
          "description": "Default normal quote, can be ' or \"."
        }
      }
    },
    "commands": [
      {
        "command": "esQuotes.switchToTemplateString",
        "title": "Switch to Template String",
        "category": "ES Quotes"
      },
      {
        "command": "esQuotes.switchToNormalString",
        "title": "Switch to Normal String",
        "category": "ES Quotes"
      },
      {
        "command": "esQuotes.switchBetweenSingleDoubleQuotes",
        "title": "Switch between Single and Double Quotes",
        "category": "ES Quotes"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "regex-tools": "^0.2.1",
    "vscode": "*"
  },
  "dependencies": {
    "typescript": "^1.8.0-dev.20151115"
  }
}
